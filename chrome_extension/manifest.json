{
	"manifest_version": 2,
	"name": "YCPasswordy",
	"version": "0.11",
	"browser_action": {
		"default_popup": "html/popup.html",
		"default_icon": "./images/techno_logo.png"
	},
	"content_scripts": [
    {
      "matches": ["http://*/*"],
      "js": ["js/main.js", "js/settings.js", "js/background.js" ]
    }
  ],

    "permissions": [
 
    "storage"
  ]
}

